<?xml version="1.0"?>
<configuration>
  <appSettings>
    <!-- Graph service endpoint -->
    <add key="AzureADServiceHost" value="graph.microsoft.com"/>
    <!-- Graph API version -->
    <add key="ApiVersion" value="v1.0"/>
    <!-- Well known service principal ID for Windows Azure AD Access Control -->
    <add key="ProtectedResourcePrincipalId" value="https://graph.microsoft.com/"/>
  </appSettings>
</configuration>